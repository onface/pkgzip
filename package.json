{
  "name": "@atlassian/morty",
  "version": "0.0.25",
  "description": "Bundles your npm packages via webpack into a single JS file",
  "main": "dist/index.js",
  "scripts": {
    "deploy": "./deploy_to_lambda.sh",
    "dist": "webpack",
    "integration-test": "npm run dist && FROG_CACHE_DISABLED=1 node ./test/bin/integration-test.js",
    "lint": "eslint src test",
    "logs": "AWS_ACCESS_KEY_ID=$FROG_AWS_ACCESS_KEY_ID AWS_SECRET_ACCESS_KEY=$FROG_AWS_SECRET_ACCESS_KEY serverless logs -f bundle -t",
    "offline": "npm run dist && FROG_CACHE_DISABLED=1 sls offline --prefix dev --noTimeout",
    "sls": "AWS_ACCESS_KEY_ID=$FROG_AWS_ACCESS_KEY_ID AWS_SECRET_ACCESS_KEY=$FROG_AWS_SECRET_ACCESS_KEY sls",
    "test": "mocha --recursive --compilers js:babel-register test/unit",
    "test/watch": "npm run test -- -w"
  },
  "keywords": [
    "npm",
    "dependency",
    "webpack"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    "public"
  ],
  "publishConfig": {
    "registry": "https://npm-private.atlassian.io"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "author": "Atlassian",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.34.0",
    "js-sha256": "^0.5.0",
    "lodash.isequal": "^4.4.0",
    "raw-loader": "^0.5.1",
    "request": "^2.81.0",
    "semver": "^5.3.0",
    "serverless": "^1.10.0",
    "tmp": "^0.0.31",
    "webpack": "^1.13.3",
    "yarn": "bengummer/yarn#lambda-fix"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.11.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.5.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "pre-commit": "^1.1.3",
    "proxyquire": "^1.7.11",
    "serverless-offline": "^3.13.0",
    "sinon": "^1.17.6",
    "webpack-node-externals": "^1.5.4"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "syntax-async-functions",
      "transform-regenerator",
      "array-includes"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "globals": {
      "logger": true
    },
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/no-exclusive-tests": "error"
    }
  }
}
