{
  "name": "@atlassian/frogmarch",
  "version": "0.0.17",
  "description": "Frogmarches your npm packages via webpack into a single JS file",
  "main": "dist/index.js",
  "scripts": {
    "dev": "babel-node -- src/index.js",
    "dev/watch": "nodemon --watch src --exec babel-node -- src/index.js",
    "dev/inspect": "nodemon --watch src --exec babel-node -- --inspect src/index.js",
    "lint": "eslint src",
    "test": "mocha --compilers js:babel-register",
    "dist": "npm run lint && babel src --out-dir dist",
    "start": "node dist/index.js",
    "nanos": "nanos service:deploy myservicename -f frogmarch.sd.yml -v --foreground -t 20000",
    "micros/publish-and-deploy/full": "npm run dist && bash dockerify.sh deploy",
    "micros/publish-and-deploy/fast": "npm run dist && bash dockerify.sh update"
  },
  "keywords": [
    "npm",
    "dependency",
    "webpack"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    "public"
  ],
  "publishConfig": {
    "registry": "https://npm-private.atlassian.io"
  },
  "author": "Atlassian",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.6.11",
    "camelcase": "^3.0.0",
    "express": "^4.14.0",
    "js-sha256": "^0.3.2",
    "nodemon": "^1.10.0",
    "tmp": "0.0.29",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.11.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.1",
    "mocha": "^3.0.2"
  }
}
