{
  "name": "@atlassian/morty",
  "version": "0.0.25",
  "description": "Bundles your npm packages via webpack into a single JS file",
  "main": "dist/index.js",
  "scripts": {
    "bamboo/staging": "FROG_AWS_ACCESS_KEY_ID=$bamboo_AWS_ACCESS_KEY_PASSWORD FROG_AWS_SECRET_ACCESS_KEY=$bamboo_AWS_SECRET_ACCESS_KEY_PASSWORD npm run deploy",
    "deploy": "docker build --build-arg AWS_ACCESS_KEY_ID=$FROG_AWS_ACCESS_KEY_ID --build-arg AWS_SECRET_ACCESS_KEY=$FROG_AWS_SECRET_ACCESS_KEY --no-cache -t lambda .",
    "dist": "webpack",
    "integration-test": "npm run dist && FROG_CACHE_DISABLED=1 AWS_ACCESS_KEY_ID=$FROG_AWS_ACCESS_KEY_ID AWS_SECRET_ACCESS_KEY=$FROG_AWS_SECRET_ACCESS_KEY node ./integration-test.js",
    "lint": "eslint src test",
    "logs": "AWS_ACCESS_KEY_ID=$FROG_AWS_ACCESS_KEY_ID AWS_SECRET_ACCESS_KEY=$FROG_AWS_SECRET_ACCESS_KEY serverless logs -f bundle -t",
    "offline": "AWS_ACCESS_KEY_ID=$FROG_AWS_ACCESS_KEY_ID AWS_SECRET_ACCESS_KEY=$FROG_AWS_SECRET_ACCESS_KEY sls offline --noTimeout",
    "sls": "AWS_ACCESS_KEY_ID=$FROG_AWS_ACCESS_KEY_ID AWS_SECRET_ACCESS_KEY=$FROG_AWS_SECRET_ACCESS_KEY sls",
    "test": "mocha --recursive --compilers js:babel-register",
    "test/watch": "npm run test -- -w"
  },
  "keywords": [
    "npm",
    "dependency",
    "webpack"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    "public"
  ],
  "publishConfig": {
    "registry": "https://npm-private.atlassian.io"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "author": "Atlassian",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.6.15",
    "camelcase": "^3.0.0",
    "js-sha256": "^0.3.2",
    "lodash.isequal": "^4.4.0",
    "morgan": "^1.7.0",
    "request": "^2.78.0",
    "semver": "^5.3.0",
    "tmp": "0.0.30",
    "webpack": "^1.13.3",
    "yarn": "bengummer/yarn#lambda-fix",
    "serverless": "1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.11.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.5.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "pre-commit": "^1.1.3",
    "proxyquire": "^1.7.10",
    "serverless-offline": "^3.3.0",
    "sinon": "^1.17.6",
    "webpack-node-externals": "^1.5.4"
  }
}
